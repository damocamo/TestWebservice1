<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>Web Test Report Log</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>Web Test Report Log</reportName>

		<!-- report headline - displayed at top-nav, after reportHeadline -->
		<reportHeadline></reportHeadline>

		<!-- global date format override -->
		<!-- defaults to yyyy-MM-dd -->
		<dateFormat>yyyy-MM-dd</dateFormat>

		<!-- global time format override -->
		<!-- defaults to HH:mm:ss -->
		<timeFormat>HH:mm:ss</timeFormat>

		<!-- custom javascript -->
		<scripts>
			<script type="text/javascript" src="http://livejs.com/live.js"></script>
			<![CDATA[
     	 $(window).bind("load", function() {
   			$(document).ready(function() {
				$('#dashboard-view').toggleClass('hide').children('div').toggleClass('hide').siblings('.charts').toggleClass('hide');
				$('#parentWithoutNodes').click();
				$('#test-count-setting .modal-close').click();
			});
		});
      ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
            <![CDATA[
     	 
      ]]>
		</styles>
	</configuration>
</extentreports>
